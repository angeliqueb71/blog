<!--  a loop around each post -->
<% @posts.each do |post| %>
 <div class="post-wrapper">
   <h2 class="title"><%= link_to post.title, post %></h2>
   <p class="date"><% post.created_at.strftime(" %B, %e, %Y ") %></p>
 </div>
<% end %>
